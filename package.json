{
  "name": "easy-dialog-modal",
  "version": "1.0.0",
  "license": "MIT",
  "author": "jamesonkunkel",
  "main": "dist/index.js",
  "module": "dist/easy-dialog-modal.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/easy-dialog-modal.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/easy-dialog-modal.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "@tsconfig/recommended": "^1.0.2",
    "@tsconfig/vite-react": "^2.0.0",
    "@types/css-modules": "^1.0.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.2.6",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "zustand": "^4.4.1"
  }
}
